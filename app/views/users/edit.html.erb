<%= form_with model: @user do |form|%>
  <%= form.label :username %>
  <%= form.text_field :username, placeholder: "Username" %>

  <%= form.label :email%>
  <%= form.text_field :email, placeholder: "yourmail@here.com" %>

  <%= form.label :password%>
  <%= form.text_field :password%>
  <%= form.submit "Submit" %>
<% end %>